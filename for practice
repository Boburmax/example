bosh obyekt yasang                                                                                          {}
obyektga misol                                                                  const man = { boy:187, rang: "sariq", series:{}}  man.rang
obyektga hususiyat berish                                                         man.series[ozg1] = ozg2
ternar operator                                shart ? hodisa : else dagi hodisa
